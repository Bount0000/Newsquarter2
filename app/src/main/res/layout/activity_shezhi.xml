<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >

    <RelativeLayout
        android:id="@+id/she_r1"
        android:background="#33ccff"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100">

        <ImageView
            android:id="@+id/shezhi_back"
            android:src="@mipmap/raw_1500023215"
            android:scaleType="fitXY"
            android:layout_centerInParent="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/x30"
            android:layout_width="@dimen/x60"
            android:layout_height="@dimen/y60" />

        <TextView
            android:layout_toRightOf="@+id/shezhi_back"
            android:textColor="#ffffff"
            android:text="返回"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y120" />

        <TextView
            android:layout_centerInParent="true"
            android:textColor="#ffffff"
            android:text="设置"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>

    <View
        android:id="@+id/she_v1"
        android:background="#EBEBEB"
        android:layout_marginTop="@dimen/x120"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x10"/>

    <RelativeLayout
        android:id="@+id/rt_banben"
        android:layout_marginTop="@dimen/x130"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120">

        <TextView
            android:id="@+id/she_tv_gengxin"
            android:gravity="center"
            android:textColor="@color/colorziti"
            android:text="检查更新"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />

        <TextView
            android:textColor="@color/colorziti"
            android:layout_centerInParent="true"
            android:layout_toLeftOf="@+id/she_img_gengxin"
            android:text="0.1.1"
            android:layout_marginRight="@dimen/x50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" />

        <ImageView
            android:id="@+id/she_img_gengxin"
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/textView4"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:gravity="center"
            android:src="@mipmap/ringht" />
    </RelativeLayout>

  <View
      android:id="@+id/view1"
      android:layout_below="@+id/rt_banben"
      android:layout_width="match_parent"
      android:layout_height="@dimen/y1"
      android:background="@color/colorziti"
      />

    <RelativeLayout
        android:id="@+id/rl_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/view1"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/she_tv_yijian"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:text="意见反馈"
            android:textColor="@color/colorziti" />

        <ImageView
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginRight="@dimen/x30"
            android:gravity="center"
            android:src="@mipmap/ringht"
            android:id="@+id/imageView" />

    </RelativeLayout>
    <View
        android:id="@+id/she_v2"
        android:background="#EBEBEB"
        android:layout_marginTop="@dimen/x430"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x35"/>

    <View
        android:layout_below="@+id/rt_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y1"
        android:background="@color/colorziti"
        />
    <RelativeLayout
        android:layout_marginTop="@dimen/x590"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:id="@+id/relativeLayout5">
        <TextView
            android:id="@+id/she_tv_huancun"
            android:gravity="center"
            android:textColor="@color/colorziti"
            android:text="清除缓存"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" />

        <TextView
            android:id="@+id/she_tv_hcnum"
            android:layout_centerInParent="true"
            android:layout_toLeftOf="@+id/she_img_huancun"
            android:text="缓存的数量"
            android:textColor="@color/colorziti"
            android:layout_marginRight="@dimen/x50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageView
            android:id="@+id/she_img_huancun"
            android:layout_marginRight="@dimen/x30"
            android:layout_centerInParent="true"
            android:src="@mipmap/ringht"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"/>
    </RelativeLayout>

    <View
        android:layout_below="@+id/relativeLayout5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y40"
        android:background="#EBEBEB"

        />
    <Button
        android:id="@+id/shezhi_login_clear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/rt_2"
        android:layout_alignRight="@+id/rt_2"
        android:layout_below="@+id/relativeLayout5"
        android:layout_marginTop="21dp"
        android:background="@drawable/redbackgroup"
        android:text="退出登录"
        android:textColor="#ffffff" />
   <LinearLayout
       android:layout_below="@+id/shezhi_login_clear"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:background="#EBEBEB"
       />

    <RelativeLayout
        android:id="@+id/rt_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x120"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/she_v2"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/she_tv_guanyu"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="11dp"
            android:gravity="center"
            android:text="关于一刻钟"
            android:textColor="@color/colorziti" />

        <ImageView
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="7dp"
            android:layout_marginEnd="7dp"
            android:layout_marginRight="7dp"
            android:gravity="center"
            android:src="@mipmap/ringht" />
    </RelativeLayout>

</RelativeLayout>
