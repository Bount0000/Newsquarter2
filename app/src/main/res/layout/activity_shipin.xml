<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lenovo.bount.newsquarter.activitybao.ShipinActivity">
    <RelativeLayout
        android:id="@+id/rl_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        >
        <ImageView
            android:id="@+id/iv_1"
            android:layout_alignParentLeft="true"
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"
            android:src="@mipmap/leftjiantou"
            />
        <TextView
             android:layout_marginLeft="@dimen/x40"
            android:id="@+id/iv_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
           android:text="返回"
            />
        <TextView
            android:id="@+id/iv_3"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="拍照"
            />
        <ImageView
            android:id="@+id/iv_4"
            android:layout_alignParentRight="true"
            android:layout_width="@dimen/x50"
            android:layout_height="@dimen/y50"
            android:src="@mipmap/ic_launcher"
            />
    </RelativeLayout>
    <ImageView
        android:id="@+id/iv_tu"
       android:layout_below="@+id/rl_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y350"
        />
    <RelativeLayout
        android:background="#A2DEFA"
        android:layout_below="@+id/iv_tu"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <ImageView
            android:id="@+id/iv_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:background="@drawable/whityuan"
            />

    </RelativeLayout>
</RelativeLayout>
